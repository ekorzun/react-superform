{
  "name": "react-superforms",
  "version": "2.0.3",
  "homepage": "https://github.com/ekorzun/react-superform",
  "author": {
    "name": "Evgeny Korzun",
    "email": "e.korzun@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekorzun/react-superform.git"
  },
  "main": "build/index.js",
  "scripts": {
    "prebuild": "rm -rf ./dist && mkdir dist",
    "build": "babel src -d build",
    "dev": "concurrently -r \"npm:type-check:watch\" \"npm:demo\"",
    "demo": "parcel serve demo/index.html",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "files": [
    "dist",
    "build"
  ],
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "chai": "^4.2.0",
    "concurrently": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.1.0",
    "jsdom": "^16.0.1",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.12.0",
    "react-json-tree-viewer": "0.0.7",
    "styled-components": "^5.0.1",
    "ts-jest": "^25.0.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  }
}
